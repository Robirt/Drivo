<?xml version = "1.0" encoding = "UTF-8" ?>
<Application
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Drivo.MAUI.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="PrimaryBackgroundColor">#FCFCFC</Color>
            <Color x:Key="SecondaryBackgroundColor">#F2F2F2</Color>
            <Color x:Key="PrimaryForegroundColor">#000000</Color>
            <Color x:Key="SecondaryForegroundColor">#252525</Color>
            <Color x:Key="AccentColor">#072b89</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>
